@using HammadBroker.Model.DTO
@model BuildingAdForDetails

@{
	ViewData["Title"] = Model.Name;
	int id = Context.Request.Query["id"].To(0);
}

@await Component.InvokeAsync(typeof(BuildingAdViewComponent), new BuildingAdModel(Model, Model)
	   {
		   Id = id,
		   CanEdit = false,
		   CanUpload = false
	   })