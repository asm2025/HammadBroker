{
	"AppName": "AqaratHammad",
	"Name": "عقارات حماد",
	"Description": "عقارات حماد للتسويق العقاري - المهندسين، 6 اكتوبر، المنصورية، طريق مصر الاسكندريه",
	"Email": "",
	"Phone": "",
	"Mobile": "+201000722872",
	"WhatsAppNumber": "+201000722872",
	"Address": "60 ميدان انس ابن مالك",
	"District": "المهندسين",
	"City": "الجيزة",
	"Website": "",
	"UseSSL": true,
	"BasePath": "/",
	"AllowedHosts": "",
	"ConnectionStrings": {
		"DefaultConnection": ""
	},
	"Migrations": {
		"ApplyMigrations": false,
		"ApplySeed": false
	},
	"OAuth": {
		"timeout": 60
	},
	"IdentityOptions": {
		"Lockout": {
			"AllowedForNewUsers": true,
			"DefaultLockoutTimeSpan": "00:05:00",
			"MaxFailedAccessAttempts": 5
		},
		"Password": {
			"RequiredLength": 8,
			"RequireDigit": true,
			"RequireNonAlphanumeric": true,
			"RequireLowercase": true,
			"RequireUppercase": true
		},
		"SignIn": {
			"RequireConfirmedAccount": true
		},
		"Store": {
			"MaxLengthForKeys": 128
		},
		"User": {
			"RequireUniqueEmail": true
		}
	},
	"VirtualPathSettings": {
		"PathContents": [
			{
				"Alias": "Images",
				"RequestPath": "/images"
			},
			{
				"Alias": "UserImages",
				"RequestPath": "/images/users"
			},
			{
				"Alias": "AssetImages",
				"RequestPath": "/images/assets"
			}
		]
	},
	"Images": {
		"BaseUrl": "images",
		"UserPlaceholder": "user.png",
		"AssetPlaceholder": "asset.png"
	},
	"ToastrOptions": {
		"Rtl": false,
		"ProgressBar": false,
		"CloseButton": true,
		"ToastClass": "toast-top-right",
		"TimeOut": 5000,
		"PreventDuplicates": true
	},
	"SmtpConfiguration": {
		"Host": "",
		"From": ""
	},
	"LoggingEnabled": true,
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft.AspNetCore": "Warning"
		}
	},
	"Serilog": {
		"Using": [
			"Serilog.Sinks.Console",
			"Serilog.Sinks.File"
		],
		"MinimumLevel": "Information",
		"Override": {
			"Microsoft": "Warning",
			"System": "Warning",
			"Microsoft.Hosting.Lifetime": "Information",
			"Microsoft.EntityFrameworkCore": "Debug"
		},
		"Enrich": [
			"FromLogContext",
			"WithMachineName",
			"WithUserName",
			"WithHttpRequestClientHostIP",
			"WithHttpRequestClientHostName",
			"WithHttpRequestRawUrl",
			"WithHttpRequestUserAgent"
		],
		"WriteTo": [
			{
				"Name": "Console"
			}
		]
	}
}
